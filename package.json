{
  "name": "agile-downloader",
  "version": "1.0.0",
  "description": "An HTML5 saveAs() AgileDownloader implementation",
  "main": "dist/AgileDownloader.min.js",
  "files": [
    "dist/AgileDownloader.js",
    "dist/AgileDownloader.min.js",
    "src/AgileDownloader.js"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "build:development": "babel -o dist/AgileDownloader.js --plugins @babel/plugin-transform-modules-umd src/AgileDownloader.js",
    "build:production": "babel -o dist/AgileDownloader.min.js -s --plugins @babel/plugin-transform-modules-umd --presets minify src/AgileDownloader.js",
    "build": "npm run build:development && npm run build:production",
    "prepublishOnly": "npm run build"
